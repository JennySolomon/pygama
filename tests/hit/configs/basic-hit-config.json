{
  "outputs": ["calE", "AoE", "A_max"],
  "operations": {
    "twice_trap_e_max": {
      "expression": "2 * trapEmax"
    },
    "calE": {
      "expression": "sqrt(a + b * twice_trap_e_max**2)",
      "parameters": {
        "a": 1.23,
        "b": 42.69
      },
      "lgdo_attrs": {
        "units": "keV",
        "hdf5_settings": {
          "compression": "gzip",
          "shuffle": true
        }
      }
    },
    "AoE": {
      "expression": "A_max/calE"
    }
  }
}
